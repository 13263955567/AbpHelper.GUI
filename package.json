{
  "name": "abphelper",
  "productName": "AbpHelper",
  "version": "0.2.0",
  "description": "AbpHelper is a tool to help you with developing Abp vNext applications.",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "package": "npm-run-all --parallel package:win64 package:mac64 package:linux64",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=ia32 --out=out --icon=assets/app-icon/mac/app.icns --asar",
    "package:mac64": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --asar",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico --asar",
    "package:win64": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=out --icon=assets/app-icon/win/app.ico --asar",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico --asar",
    "package:linux64": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out --icon=assets/app-icon/win/app.ico --asar",
    "package:sign-exe": "signcode './out/AbpHelper-win32-ia32/AbpHelper.exe' --cert ~/AbpHelper.p12 --prompt --name 'AbpHelper' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/AbpHelper.p12 --prompt --name 'AbpHelper' --url 'http://electron.atom.io'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": "https://github.com/EasyAbp/AbpHelper.GUI",
  "keywords": [
    "EasyAbp",
    "Abp",
    "Helper"
  ],
  "author": "EasyAbp",
  "license": "Apache 2.0",
  "devDependencies": {
    "@octokit/rest": "^17.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "check-for-leaks": "^1.2.1",
    "devtron": "^1.4.0",
    "electron-packager": "^14.2.1",
    "electron-winstaller": "^4.0.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "signcode": "^1.0.0",
    "spectron": "^10.0.1",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "electron": "^8.1.0",
    "electron-log": "^4.0.7",
    "electron-main-fetch": "^0.2.0",
    "electron-settings": "^3.2.0",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^7.1.6",
    "highlight.js": "^9.18.1",
    "update-electron-app": "^1.5.0"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
